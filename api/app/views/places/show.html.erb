  <div id="location-rate-div">
    <h3 id="location-rate"> <%= @place.name %> </h3>
  </div>
  <br>
  <div>
    <h3> Current Rating: </h3>
  </div>
  <div id="current-rating-div">
    <h3 id="current-rating"> <%= @occupancy_now %> </h3>
  </div>
  <br>

  <div class="span7 text-center">
    <%= button_to "Rate", edit_place_path(@place), class: "btn btn-primary btn-lg", method: :get %>
  </div>


  <br>
  <div class="graph_fancy">
    <span class="bar"></span>
    <% @occupancy_day.occupancies.zip(@hours) do |occupancy, hour| %>
      <span class="bar" style="height:<%= occupancy*20 %>%"; data-bar-label="<%= hour %>" data-bar-value="<%= occupancy %>"></span>
    <% end %>
  </div>

  <div id="graph-axis">
    <h3 id="graph-x-axis"> Time of Day </h3>
  </div>
  <br>
  <br>

  <div id="location-rate-div">
    <h3 id="location-rate"> Address </h3>
  </div>
  <br>

  <div id="address-div">
    <h3 id="address"> <%= @place.address %> </h3>
  </div>
  <br>

  <div id="location-rate-div">
    <h3 id="location-rate"> Phone </h3>
  </div>
  <br>

  <div id="phone-num-div">
    <h3 id="phone-num"> 534-342-4323 </h3>
  </div>
  <br>

  <% if session[:user] %>
    <div class="add-to-favorites-div">
        <h3 class="add-to-favorites"> Add to favorites! </h3>
    </div>
    <div class="span7 text-center">
        <%= form_tag user_favorites_path(user_id: session[:user]["id"], place_id: @place.id) do %>
            <%= submit_tag "Favorite", id: "check-map-button", class: "btn btn-primary btn-lg" %>
        <% end %>
    </div>
<% end %>